script/source = "extends PanelContainer

onready var main_tab = get_parent()

onready var Multiplayer = Global.get_node(\"Multiplayer\")
onready var NetworkBridge = Global.get_node(\"Multiplayer/NetworkBridge\")

onready var SteamLobby = Global.get_node(\"Multiplayer/SteamInit/SteamLobby\")
onready var SteamInit = Global.get_node(\"Multiplayer/SteamInit\")
onready var SteamNetwork = Global.get_node(\"Multiplayer/SteamInit/SteamNetwork\")

onready var playersList = $VBoxContainer/PlayersList/PlayersListLabel

onready var LobbyList = $VBoxContainer/PlayersList/LobbyList
onready var LobbyInfo = $VBoxContainer/LobbyInfo

onready var status = $VBoxContainer/Status/StatusLabel

var lobbies = []
var selected = 0

var join_request_id = 0

func _ready():
	SteamLobby.connect(\"lobby_list\", self, \"_on_match_list\")
	
	SteamLobby.connect(\"player_joined_lobby\", self, \"player_list_update\")
	SteamLobby.connect(\"player_left_lobby\", self, \"player_list_update\")
	
	SteamLobby.connect(\"lobby_joined\", self, \"player_list_update\")
	
	SteamLobby.connect(\"lobby_join_requested\", self, \"lobby_join_requested\")
	
	Multiplayer.connect(\"status_update\", self, \"update_status\")
	
	$\"%PasswordInput\".connect(\"password_set\", self, \"connect_lobby\")
	
	SteamNetwork.register_rpcs(self,[
		[\"_rpc_client\", SteamNetwork.PERMISSION.SERVER],
		[\"_rpc_server\", SteamNetwork.PERMISSION.CLIENT_ALL]
	])

func create_lobby():
	SteamLobby.create_lobby(2, 16)
	$VBoxContainer/Buttons/Create.hide()
	$VBoxContainer/Buttons/Join.hide()
	$VBoxContainer/Buttons/Refresh.hide()
	$VBoxContainer/Buttons/Leave.show()
	
	$VBoxContainer/PlayersList/PlayersListLabel.show()
	LobbyList.hide()
	LobbyInfo.hide()

func lobby_join_requested(id):
	NetworkBridge.set_mode(NetworkBridge.MULTIPLAYER_TYPE.STEAM)
	main_tab.current_tab = 2
	
	join_request_id = int(id)
	print(\"[CRUS ONLINE / LOBBY]: Lobby join request id: \", str(join_request_id))
	
	var password_require = SteamInit.Steam.getLobbyData(join_request_id, \"password\")
	print(\"[CRUS ONLINE / LOBBY]: Password require: \", password_require)
	if password_require == \"true\":
		join_request_id = id
		$\"%PasswordInput\".steam_check_password()
	else:
		connect_lobby()

func join_lobby():
	var password_require = SteamInit.Steam.getLobbyData(lobbies[selected], \"password\")
	print(\"[CRUS ONLINE / LOBBY]: Password require: \", password_require)
	if password_require == \"True\":
		$\"%PasswordInput\".steam_check_password()
	else:
		connect_lobby()

func show_player_list():
	$VBoxContainer/PlayersList/PlayersListLabel.show()

func connect_lobby():
	if join_request_id != 0:
		SteamLobby.join_lobby(join_request_id)
		join_request_id = 0
	else:
		SteamLobby.join_lobby(lobbies[selected])
	
	$VBoxContainer/Buttons/Create.hide()
	$VBoxContainer/Buttons/Join.hide()
	$VBoxContainer/Buttons/Refresh.hide()
	$VBoxContainer/Buttons/Leave.show()
	
	Multiplayer.emit_signal(\"status_update\", \"Connecting...\")
	
	#$VBoxContainer/PlayersList/PlayersListLabel.show()
	LobbyList.hide()
	LobbyInfo.hide()

func leave_lobby():
	SteamLobby.leave_lobby()
	Multiplayer.emit_signal(\"status_update\", \"Offline\")
	$VBoxContainer/Buttons/Create.show()
	$VBoxContainer/Buttons/Join.show()
	$VBoxContainer/Buttons/Refresh.show()
	$VBoxContainer/Buttons/Leave.hide()
	
	$VBoxContainer/PlayersList/PlayersListLabel.hide()
	LobbyList.show()
	LobbyInfo.show()

func lobby_selected(id):
	print(id)
	selected = id
	
	var lobby_max_players = SteamInit.Steam.getLobbyMemberLimit(lobbies[selected])
	var lobby_num_members = SteamInit.Steam.getNumLobbyMembers(lobbies[selected])
	var password_require = SteamInit.Steam.getLobbyData(lobbies[selected], \"password\")
	
	LobbyInfo.text = \"Players: \" + str(lobby_num_members) + \" / \" + str(lobby_max_players) + \"   Password: \" + str(password_require)

func get_lobbies():
	SteamLobby.request_lobby_list()

func _on_match_list(recived_lobbies):
	print(\"[CRUS ONLINE / STEAM] Recived a lobbies list\")
	LobbyList.clear()
	var raw_lobbies = []
	
	for this_lobby in recived_lobbies:
		if SteamInit.Steam.getLobbyData(this_lobby, \"crus_online\"):
			raw_lobbies.append(this_lobby)
			
			var lobby_name = SteamInit.Steam.getLobbyData(this_lobby, \"name\")
			var lobby_max_players = SteamInit.Steam.getLobbyMemberLimit(this_lobby)
			var lobby_num_members = SteamInit.Steam.getNumLobbyMembers(this_lobby)
			var password_require = SteamInit.Steam.getLobbyData(lobbies[selected], \"password\")
			
			LobbyList.add_item(lobby_name)
	
	lobbies = raw_lobbies

func player_list_update(placeholder):
	print(\"[CRUS ONLINE / STEAM] player list update\")
	var players = SteamLobby.get_lobby_members()
	playersList.bbcode_text = \"\"
	
	var playerNumber = 1
	
	for player in players:
		playersList.bbcode_text += str(playerNumber) + \": \" + players[player]
		
		if SteamLobby.is_owner(player):
			playersList.bbcode_text += \" (host)\\n\"
		else:
			playersList.bbcode_text += \"\\n\"
		
		playerNumber += 1

func back_to_menu():
	main_tab.current_tab = 0
	leave_lobby()

func update_status(new_status):
	status.text = new_status
	
	if new_status == \"Connected to Lobby\":
		show_player_list()

func _rpc_test():
	if NetworkBridge.check_connection():
		print(\"[CRUS ONLINE / STEAM / TEST]: connection - true\")
		print(\"[CRUS ONLINE / STEAM / TEST]: peers - \", NetworkBridge.get_peers())
		if NetworkBridge.r_is_network_master(self):
			print(\"[CRUS ONLINE / STEAM / TEST]: sending rpc, type - server\")
			NetworkBridge.r_rpc(self, \"_rpc_client\")
		else:
			print(\"[CRUS ONLINE / STEAM / TEST]: sending rpc, type - client\")
			NetworkBridge.r_rpc(self, \"_rpc_server\")
	else:
		print(\"[CRUS ONLINE / STEAM / TEST]: connection - false\")

master func _rpc_server(id):
	print(\"[CRUS ONLINE / STEAM / TEST]: recived rpc from client\")

puppet func _rpc_client(id):
	print(\"[CRUS ONLINE / STEAM / TEST]: recived rpc from server\")
"

