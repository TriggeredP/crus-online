script/source = "extends PanelContainer

var gamemodesList = [
	[\"Cruelty\",\"Gamemode without any special goals, kill each other until you get bored, is the standard mode for playing Deathmatch and CO-OP maps\"]
]

var mapsList = [
	\"res://MOD_CONTENT/CruS Online/maps/bonesquad_museum.tscn\",
	\"res://MOD_CONTENT/CruS Online/maps/Construct_Blam.tscn\",
	\"res://MOD_CONTENT/CruS Online/maps/edenrat.tscn\",
	\"res://MOD_CONTENT/CruS Online/maps/multiplayer_test_map.tscn\"
]

var mapsName = [
	\"Bonequad museum\",
	\"Construct\",
	\"Edenrat\",
	\"Test map\"
]

func _ready():
	for gamemode in gamemodesList:
		$HBoxContainer/LevelController/GamemodeList.add_item(gamemode[0])
	
	for map in mapsName:
		$HBoxContainer/LevelController/MapList.add_item(map)
	
	$HBoxContainer/LevelController/GamemodeList.select(0)
	$HBoxContainer/LevelController/MapList.select(0)
	
	_gamemode_select(0)
	_map_select(0)


func _gamemode_select(index):
	get_parent().get_node(\"Main\").set_gamemode_label(gamemodesList[index][0])
	get_tree().get_nodes_in_group(\"MultiplayerMenu\")[0].hostInfo[\"gamemode\"] = gamemodesList[index][0]

	$HBoxContainer/GamemodeDescription/MapLabel.text = \"Gamemode description:\\n\\n\" + gamemodesList[index][1]

func _map_select(index):
	get_parent().get_node(\"Main\").set_map_label(mapsList[index])
	get_tree().get_nodes_in_group(\"MultiplayerMenu\")[0].hostSettings[\"map\"] = mapsList[index]
"

[sub_resource type="GDScript" id=13]
script/source = "extends PanelContainer

var bannedImplants = {}

onready var allowedImplantsList = $HBoxContainer/ImplantsController/AllowedImplantsList

var updated = false

func update():
	updated = true
	
	for index in len(Global.implants.IMPLANTS):
		if Global.implants.IMPLANTS[index].i_name != \"House\":
			allowedImplantsList.add_item(Global.implants.IMPLANTS[index].i_name,Global.implants.IMPLANTS[index].texture,false)

func toggle_implant(index):
	if not allowedImplantsList.is_item_disabled(index):
		bannedImplants[allowedImplantsList.get_item_text(index)] = index
		allowedImplantsList.set_item_disabled(index,true)
		$Unequip.play()
	else:
		bannedImplants.erase(allowedImplantsList.get_item_text(index))
		allowedImplantsList.set_item_disabled(index,false)
		$Equip.play()

func allow_all():
	bannedImplants = {}
	
	for index in len(Global.implants.IMPLANTS):
		if Global.implants.IMPLANTS[index].i_name != \"House\":
			allowedImplantsList.set_item_disabled(index,false)
	
	$Equip.play()

func ban_all():
	bannedImplants = {}
	
	for index in len(Global.implants.IMPLANTS):
		if Global.implants.IMPLANTS[index].i_name != \"House\":
			bannedImplants[allowedImplantsList.get_item_text(index)] = index
			allowedImplantsList.set_item_disabled(index,true)
	
	$Unequip.play()
"

