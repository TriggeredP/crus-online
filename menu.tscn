script/source = "extends PanelContainer

onready var httpRequest = $HTTPRequest

func _ready():
	httpRequest.connect(\"request_completed\", self, \"_http_request_completed\")
	
	var err = httpRequest.request(\"https://triggeredp.site/api/boosty\")
	if err != OK:
		push_error(\"[CRUS ONLINE / MENU / CREDITS]: An error occurred in the boosty HTTP request.\")

func _http_request_completed(result, response_code, headers, body):
	var response = parse_json(body.get_string_from_utf8())
	
	if response.type == \"boosty\":
		$RichTextLabel.bbcode_text += \"[center] Boosty subs: [/center]\\n\"
		
		for subType in response.data:
			$RichTextLabel.bbcode_text += \"\\n> \" + subType.name + \":\\n\"
			for sub in subType.subs:
				$RichTextLabel.bbcode_text += sub + \"\\n\"

func meta_clicked(meta):
	OS.shell_open(str(meta))
"

