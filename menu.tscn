script/source = "extends HBoxContainer

func r_change(text):
	if int(text) > 255: text = \"255\"
	if int(text) < 0: text = \"0\"
	
	$Red.text = str(int(text))
	$ColorRect.color.r8 = int(int(text))

func g_change(text):
	if int(text) > 255: text = \"255\"
	if int(text) < 0: text = \"0\"
	
	$Green.text = str(int(text))
	$ColorRect.color.g8 = int(int(text))

func b_change(text):
	if int(text) > 255: text = \"255\"
	if int(text) < 0: text = \"0\"
	
	$Blue.text = str(int(text))
	$ColorRect.color.b8 = int(int(text))

func focus_exited():
	r_change($Red.text)
	g_change($Green.text)
	b_change($Blue.text)
"

