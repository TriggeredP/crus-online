script/source = "extends PanelContainer

onready var playersList = $VBoxContainer/PlayersList/PlayersListLabel
onready var status = $VBoxContainer/Status/StatusLabel

func _ready():
	playersList.bbcode_text = \"\"
	
	get_tree().get_nodes_in_group(\"Multiplayer\")[0].connect(\"status_update\", self, \"update_status\")
	get_tree().get_nodes_in_group(\"Multiplayer\")[0].connect(\"players_update\", self, \"update_players_info\")

func update_status(new_status):
	status.text = new_status

func update_players_info(info):
	var playerNumber = 1
	playersList.bbcode_text = \"\"
	
	for player in info:
		print(info[player])
		playersList.bbcode_text += str(playerNumber) + \": [color=#\" + info[player].color + \"]\" + info[player].nickname + \"[/color]\"
		
		if player == 1:
			playersList.bbcode_text += \" (host)\\n\"
		else:
			playersList.bbcode_text += \"\\n\"
		
		playerNumber += 1
"

