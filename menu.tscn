script/source = "extends WindowDialog

onready var Multiplayer = get_tree().get_nodes_in_group(\"Multiplayer\")[0]

onready var errorLabel = $Panel/MarginContainer/RichTextLabel

var errorText = [
	\"[center]Unknow error[/center]\",
	\"[center]Time out[/center]\",
	\"[center]The password is incorrect[/center]\",
	\"[center]The client and host mod versions do not match[/center]\"
]

func _ready():
	rect_scale = Vector2(Global.resolution[0] / 1280 ,Global.resolution[1] / 720 )
	
	Multiplayer.connect(\"disconnected_from_server\", self, \"show_error\")

func show_error(error):
	if error != Multiplayer.errorType.PASSWORD_REQUIRE:
		rect_scale = Vector2(Global.resolution[0] / 1280 ,Global.resolution[1] / 720 )
		
		errorLabel.bbcode_text = errorText[error]
		popup_centered()
"

